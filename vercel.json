{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/travel.png",
        "dest": "/travel.png"
      },
      {
        "src": "/api/opentrip",
        "dest": "/api/opentrip"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  